# Basic Sway Configuration
set $mod Mod1

default_border none
default_floating_border none
focus_follows_mouse no

swaybg_command -

#output * res 1920x1080 bg #000000 solid_color

output * disable
output HDMI-A-2 enable
output HDMI-A-2 resolution 1920x1080 position 0,0 transform 90 bg #000000 solid_color


for_window [app_id="mpv"] {
    floating enable
    sticky enable
}

exec /root/stream-viewer/streams.sh